#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules/.pnpm/elm-format@0.8.5/node_modules/elm-format/bin/node_modules:/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules/.pnpm/elm-format@0.8.5/node_modules/elm-format/node_modules:/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules/.pnpm/elm-format@0.8.5/node_modules:/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules/.pnpm/node_modules:/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules:/mnt/d/Dev/Spielwiese/ELM/node_modules:/mnt/d/Dev/Spielwiese/node_modules:/mnt/d/Dev/node_modules:/mnt/d/node_modules:/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules/elm-format/bin/node_modules:/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules/elm-format/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules/.pnpm/elm-format@0.8.5/node_modules/elm-format/bin/node_modules:/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules/.pnpm/elm-format@0.8.5/node_modules/elm-format/node_modules:/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules/.pnpm/elm-format@0.8.5/node_modules:/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules/.pnpm/node_modules:/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules:/mnt/d/Dev/Spielwiese/ELM/node_modules:/mnt/d/Dev/Spielwiese/node_modules:/mnt/d/Dev/node_modules:/mnt/d/node_modules:/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules/elm-format/bin/node_modules:/mnt/d/Dev/Spielwiese/ELM/bootstrap/node_modules/elm-format/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../elm-format/bin/elm-format" "$@"
else
  exec node  "$basedir/../elm-format/bin/elm-format" "$@"
fi
